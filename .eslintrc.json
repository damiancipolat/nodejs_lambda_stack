{
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "quote-props":"off",
        "import/newline-after-import":"off",
        "import/order":"off",
        "spaced-comment":"off",
        "global-require":"off",
        "arrow-body-style":"off",
        "comma-spacing":"off",
        "indent":"off",
        "eol-last":"off",
        "comma-dangle":"off",
        "linebreak-style":"off",
        "no-console": "off",
        "no-trailing-spaces":"off",
        "padded-blocks":"off"
    }
}